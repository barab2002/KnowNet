{
  "name": "@barab2002/knownet",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/barab2002/KnowNet.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "docker compose up -d && cross-env NODE_ENV=development nx run-many --targets=serve --all",
    "dev:down": "docker compose down",
    "dev:api": "cross-env NODE_ENV=development npx nx serve api",
    "dev:client": "nx serve client",

    "prod:build": "nx run-many --targets=build --all",
    "prod:start": "pm2 start ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:restart": "pm2 restart ecosystem.config.js",
    "prod:logs": "pm2 logs",
    "prod:status": "pm2 status",

    "test": "nx test api",
    "test:watch": "nx test api --watch",
    "test:coverage": "nx test api --code-coverage",
    "e2e:client": "cross-env NX_DAEMON=false NX_OUTPUT_STYLE=static CI=1 nx e2e client-e2e --browser electron --skip-nx-cache",
    "e2e:client:smoke": "cross-env NX_DAEMON=false NX_OUTPUT_STYLE=static CI=1 nx e2e client-e2e --browser electron --skip-nx-cache --spec client-e2e/src/e2e/app.cy.ts",
    "e2e:client:open": "nx run client-e2e:open-cypress"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
